{
    "name": "vrijplaatsleiden/voordekunst-crawler",
    "type": "library",
    "description": "vdkcrawler",
    "keywords": [
        "vrijplaatsleiden",
        "voordekunst-crawler"
    ],
    "homepage": "https://github.com/vrijplaatsleiden/voordekunst-crawler",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronald",
            "email": "ronald@vrijplaatsleiden.nl",
            "homepage": "https://vrijplaatsleiden.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "ext-curl": "*",
        "symfony/dom-crawler": "~3.1",
        "symfony/css-selector": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0"
    },
    "autoload": {
        "psr-4": {
            "Vpl\\VdkCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vpl\\VdkCrawler\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
